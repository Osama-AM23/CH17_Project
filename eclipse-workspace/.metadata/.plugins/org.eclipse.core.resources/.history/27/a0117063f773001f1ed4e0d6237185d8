package com.xworkz.procedure.internal;

public class NimansHospital implement HospitalRule {
	
	@Override
	default int noOfVisiters() {
		System.out.println("Execute noOFVisiters in NimansHospital");
		return 1;
	}
	
	@Override
	default boolean noSmoking() {
		System.out.println("Execute noSmoking in NimansHospital");
		return false;
	}
	
	@Override
	default boolean beSilent() {
		System.out.println("Execute beSilent in NimansHospital");
		return false;
	}
}
