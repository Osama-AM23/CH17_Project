package com.xworkz.procedure.internal;

public interface TrafficRule {
	
	boolean licence();
	
	boolean helmet();
	
	boolean numberPlate();
	
	double speedLimit();
	
	int noOfPerson();
	
	boolean  directionIndicator();
	
	boolean insurance();
	
	boolean rcBook();
	
	boolean seatBelt();
	
	boolean noSmoking();
	
	boolean noDrink();
	
	boolean noParkToPublicPlace();
	
	boolean noModification();
	
	boolean ambulanceFreePassage();
	
	boolean useHornInSilentZone();
	
	

}
